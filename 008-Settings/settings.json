  "debug.confirmOnExit": "always",
  "window.confirmBeforeClose": "always",
  "window.zoomLevel": -1,

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "strings": "on"
  },

  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,

  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.tabs.enabled": false,

  "workbench.colorTheme": "Monokai Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.editor.enablePreview": false,
  "workbench.welcomePage.walkthroughs.openOnInstall": false,

  "search.maxResults": 280000,
  "security.workspace.trust.untrustedFiles": "open",

  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },

  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",

  "material-icon-theme.activeIconPack": "react",
  "prettier.singleQuote": true,
  "prettier.printWidth": 90,

  "projectManager.git.baseFolders": ["~/Projects"],
  "hediet.vscode-drawio.theme": "min",
  "redhat.telemetry.enabled": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "reactSnippets.settings.importReactOnTop": false,

  "python.condaPath": "/opt/homebrew/Caskroom/miniconda/base/envs/base2",
  "python.formatting.provider": "none",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,

  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },

  "files.exclude": {
    "**/__pycache__": true
  },

  "go.formatTool": "goimports",
  "go.useLanguageServer": false,
  "go.toolsManagement.autoUpdate": true,

  "[go]": {
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "golang.go"
  },

  "files.associations": {
    "*.gohtml": "html",
    "*.css": "tailwindcss"
  },

  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },

  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },

  "protoc": {
    "options": ["--proto_path=protos/v3"]
  }
}
